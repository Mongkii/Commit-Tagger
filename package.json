{
  "name": "commit-tagger",
  "displayName": "Commit Tagger",
  "description": "Generate tag for your commit at ease.",
  "version": "2.0.1",
  "publisher": "Mongkii",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "git",
    "commit",
    "tag",
    "commitizen"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/mongkii/commit-tagger/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongkii/commit-tagger"
  },
  "homepage": "https://github.com/mongkii/commit-tagger/blob/master/README.md",
  "activationEvents": [
    "onCommand:extension.commitTagger",
    "onCommand:extension.commitTaggerSettings"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.commitTagger",
        "title": "Commit Tagger",
        "icon": {
          "dark": "resources/dark/tag.svg",
          "light": "resources/light/tag.svg"
        }
      },
      {
        "command": "extension.commitTaggerSettings",
        "title": "Commit Tagger Settings"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "when": "scmProvider == git",
          "command": "extension.commitTagger",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.commitTagger",
        "key": "shift+alt+c",
        "mac": "cmd+ctrl+c"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.17.18",
    "@types/vscode": "^1.30.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "glob": "^7.1.6",
    "mocha": "^6.2.3",
    "prettier": "^2.0.2",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "eslintIgnore": [
    "webview/neverland.js"
  ]
}
